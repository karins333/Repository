import itertools

def signed_permutations(n):
    numbers = list(range(1, n + 1))
    perms = itertools.permutations(numbers)  # Все перестановки без знаков
    signed_perms = []

    for perm in perms:
        # Генерируем все возможные комбинации знаков для данной перестановки
        for signs in itertools.product([-1, 1], repeat=n):
            signed_perm = [str(sign * num) for sign, num in zip(signs, perm)]
            signed_perms.append(" ".join(signed_perm))

    return signed_perms

n = int(input())
signed_perms = signed_permutations(n)
print(len(signed_perms))
for perm in signed_perms:
    print(perm)